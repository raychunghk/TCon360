{
  "name": "@tcon360/config",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.10.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.17.30",
    "typescript": "^5.5.3"
  }
}
