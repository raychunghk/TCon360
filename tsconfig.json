{
  "compilerOptions": {
    "jsx": "preserve",
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "moduleResolution": "Node",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "middleware.ts",
    "**/*.ts",
    "**/*.tsx",
    "./.eslintrc.js",
    "pages/timesheet/create.jsx.backup",
    "src/server/leaverequest/leaverequest.resolver.ts",
    "pages/leaverequest/index.jsx",
    "pages/_document.jsx",
    "pages/api/auth/[...nextauth].js",
    "pages/user/signup.tsx",
    "components/Signupcard.js",
    "src/server/timesheet/service/timesheet.service.ts.nouse"
  ],
  "exclude": [
    "node_modules"
  ]
}